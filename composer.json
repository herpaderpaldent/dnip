{
    "name": "herpaderpaldent/dnip",
    "description": "Prove of concept regarding https://dnip.ch/2021/12/09/gespraechiges-urner-grundbuchregister/.",
    "keywords": ["poc", "dnip", "cli"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Huber",
            "email": "felix.a.huber@gmx.net"
        }
    ],
    "require": {
        "php": "^8.0",
        "kitetail/zttp": "^0.6.0",
        "laravel-zero/framework": "^8.8",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["dnip"]
}
